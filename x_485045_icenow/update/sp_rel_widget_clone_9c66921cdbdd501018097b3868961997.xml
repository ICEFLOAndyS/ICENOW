        <payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;sp_widget&gt;&lt;category/&gt;&lt;client_script&gt;&lt;![CDATA[function($scope, PWGanttConfig, PCService, $rootScope) {
	/* widget controller */
	var c = this;
	
	$scope.savePreferenceAndSettings = function(mode){
		if (mode == 'beforestart')
			PWGanttConfig.MODE = "introduction";
		else PWGanttConfig.MODE = "";
		PCService.savePreference('INTRO', true, true)
			.then(function() {
			PWGanttConfig.SETTINGS.INTRO = true;
		});
		if(mode != 'beforestart')
			$rootScope.$broadcast('walkthrough.closed');
	};
}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css/&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description&gt;Walkthrough for planning console using introjs&lt;/description&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;false&lt;/has_preview&gt;&lt;id&gt;callout&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link&gt;&lt;![CDATA[function(scope, ele, attr) {
	var PWGanttInstance = $injector.get('PWGanttInstance'),
		PWConstants = $injector.get('PWConstants'),
		PWConfig = $injector.get('PWConfig'),
		ProjectGanttWalkthroughSteps = $injector.get('ProjectGanttWalkthroughSteps'),
		$window = $injector.get('$window'),
		$timeout = $injector.get('$timeout'),
		PWGanttConfig = $injector.get('PWGanttConfig');
	
	var gantt = PWGanttInstance.get(PWConstants.GANTT_INSTANCE);
	if(gantt &amp;&amp; typeof gantt.scrollTo === 'function') gantt.scrollTo(0, 0);
	if(PWConfig.entity === PWConstants.ENTITY.PROJECT) {
		scope.steps = ProjectGanttWalkthroughSteps.getSteps();
		
		var callout = new Callout({
			steps: scope.steps,
			callbacks: {
				beforeStart: _closeMenuAndSavePreference,
				onExit: _closeMenuAndSavePreference,
				onComplete: _closeMenuAndSavePreference
			},
			welcomeScreen: {
				intro: "&lt;b&gt;" + scope.data.messages.planningConsole + "&lt;/b&gt;&lt;br/&gt;" +
						scope.data.messages.pcOverView + scope.data.messages.liveFeed,
				introShort: scope.data.messages.introShort
			},
			startLabel: scope.data.messages.startLabel,
			nextLabel: scope.data.messages.nextLabel,
			doneLabel: scope.data.messages.doneLabel
		});

		$timeout(function() {
			callout.start(PWGanttConfig &amp;&amp; PWGanttConfig.SETTINGS &amp;&amp; PWGanttConfig.SETTINGS.showWelcomeScreen);
		}, 0);

	} 
	
	function _closeMenuAndSavePreference(mode) {
		scope.savePreferenceAndSettings(mode);
		$('body').trigger('click');
	}
}]]&gt;&lt;/link&gt;&lt;name&gt;Callout&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {
	/* populate the 'data' object */
	/* e.g., data.table = $sp.getValue('table'); */
	data.messages = {};
	data.messages.planningConsole = gs.getMessage('Planning Console');
	data.messages.pcOverView = gs.getMessage('The planning console gives project managers a comprehensive view of all aspects of a project, including a hierarchical list of sub-projects, if any, project tasks, which appear in a work breakdown structure (WBS) list with task assignments, and the project Gantt chart. ');
	data.messages.liveFeed = gs.getMessage('The console also integrates with Live Feed so your users can collaborate on projects.');
	data.messages.introShort = gs.getMessage('Introduction to Planning Console.');
	data.messages.startLabel = gs.getMessage('Take the tour');
	data.messages.nextLabel = gs.getMessage('Got it');
	data.messages.doneLabel = gs.getMessage('Done');
})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2016-11-13 23:53:50&lt;/sys_created_on&gt;&lt;sys_id&gt;d95e858a0b472200b7d0c32c15673a55&lt;/sys_id&gt;&lt;sys_mod_count&gt;68&lt;/sys_mod_count&gt;&lt;sys_name&gt;Callout&lt;/sys_name&gt;&lt;sys_package display_value="Project Portfolio Suite Portal" source="com.snc.pps_portal"&gt;a614b505db09101018097b38689619fc&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_d95e858a0b472200b7d0c32c15673a55&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2018-11-01 12:21:49&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;now-message key="Switch between views" value="${Switch between planning console and workbench views for the current project}"&gt;&lt;/now-message&gt;
