        <script><![CDATA[function approveRunbook()
{
	var ga = new GlideAjax("RunbookStateHandlerAjax");         
    ga.addParam("sysparm_name", "getStateValue");         
    ga.addParam("sysparm_state_name", "approved"); 
    ga.getXMLAnswer(function(stateValue) {              
    g_form.setValue("state", stateValue);              
    gsftSubmit(null, g_form.getFormElement(), "state_model_move_to_approved");         
    });
} 
          if (typeof window == 'undefined')   
             setRedirect(); 

function setRedirect() 
{   
    current.update();   
    action.setRedirectURL(current);
}]]></script>
